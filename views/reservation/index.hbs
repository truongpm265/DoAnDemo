<h1 class="container text-center">Room List</h1>


<table class="table table-primary">
   <tr>
      <th>Room</th>
      <th>Name</th>
      <th>Check-In</th>
      <th>Check-Out</th>
      <th>Total Price</th>
      <th>Status</th>
   </tr>
   {{#each reservationList }}
   <tr>
      <td>{{ room.roomNumber }}</td>
      <td>{{ user.username }}</td>
      <td>{{ checkInDate }}</td>
      <td>{{ checkOutDate }}</td>
      <td>${{ totalPrice }}</td>
      <td>
         
      </td>
   </tr>
   {{/each}}
</table>